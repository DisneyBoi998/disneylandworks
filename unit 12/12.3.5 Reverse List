function main() {
    let nums = [1, 2, 3, 4];
	let names = ["Pete", "Terry", "Rosa", "Jason"];

    let numsReversed = reverseList(nums);
	let namesReversed = reverseList(names);
	
    console.log(nums);
	console.log(numsReversed);
    console.log(names);
    console.log(namesReversed);
}

// This list reverses the parameter and returns it 
function reverseList(arr) {
    let newArr = [];

    for (let i = arr.length - 1; i >= 0; i--) {
        newArr.push(arr[i]);
    }

    return newArr;
}

main();
