function main() {
    addImage();
    addText();
}
main();

function addImage(){
    let copter= new WebImage("https://static.codehs.com/img/library/objects/helicopter.png");
copter.setSize(250, 250);
copter.setPosition(getWidth()/4, getHeight()/2); 
add(copter);
}
function addText(){
    let txt = new Text("ahhhhhh", "30pt Arial");
    txt.setPosition(150,50);
    txt.setColor("red");
    add(txt);
}
