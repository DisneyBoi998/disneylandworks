/* This program will have Karel run around the racetrack
 * 8 times. */
function start() {
    // Write your code here
    for (var i = 0; i < 32; i++) {
        neededFunction1();
        neededFunction2();
    }
}

function neededFunction1() {
    while(frontIsClear()){
        move();
    }
}

function neededFunction2() {
    if(frontIsBlocked()){
        putBall();
        turnLeft();
    }
}
