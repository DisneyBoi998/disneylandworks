function start(){
    goUp();
    goDown();
    goUp();
}

function goDown() {
    move();
    turnRight();
    move();
    move();
    move();
    turnLeft();
}

function goUp() {
    move();
    putBall();
    turnLeft();
    move();
    putBall();
    move();
    putBall();
    move();
    turnRight();
}
function turnRight() {
    turnLeft();
    turnLeft();
    turnLeft();
}
