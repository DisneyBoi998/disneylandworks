function start(){
var partOfDay= "morning";
var time= 10;
println("In the " + partOfDay + ", I eat breakfast at "+ time + ".");
partOfDay= "afternoon";
time= 3;
println("In the " + partOfDay + ", I eat lunch at "+ time + ".");
partOfDay= "evening";
time= 8;
println("In the " + partOfDay + ", I eat dinner at " + time + ".");
}
